<%- include('../partials/nav') %>

<h1>Update Employee</h1>
<form method="POST" action="/employees/<%= employee._id %>?_method=PUT">
<label>Employee Name:
  <input type="text" name="eName" autocomplete="off"><br>
</label>
<label>Start Date:
  <input type="date" name="startDate" autocomplete="off"><br>
</label>
<label>Title:
  <input type="text" name="title" autocomplete="off"><br>
</label>
<label>Department:
  <input type="text" name="department" autocomplete="off"><br>
</label>
<label>Location:
  <input type="text" name="location" autocomplete="off"><br>
</label>
<button type="submit">Update</button>
</form>

